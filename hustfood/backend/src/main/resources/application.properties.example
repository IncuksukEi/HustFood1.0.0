spring.application.name=hustfoodv1
spring.datasource.url=jdbc:mysql://127.0.0.1:3306/hustfood
spring.datasource.username=root
spring.datasource.password=Anhhdfg123@

spring.security.user.name=admin
spring.security.user.password=123456

jwt.secret=this_is_a_very_secret_key_1234567890!@#$
jwt.expiration=36000000

spring.datasource.driver-class-name=com.mysql.cj.jdbc.Driver
spring.jpa.hibernate.ddl-auto=update
spring.jpa.open-in-view=false

# OpenAI Configuration
openai.api.key=${OPENAI_API_KEY}
openai.api.url=https://api.openai.com/v1/chat/completions
openai.model=${OPENAI_MODEL}
openai.embedding.model=${OPENAI_EMBEDDING_MODEL}
openai.temperature=0.5
openai.max-tokens=1000

# Qdrant Configuration
qdrant.host=localhost
qdrant.port=6333
qdrant.collection.name=hustfood
qdrant.vector.size=1536
qdrant.score.threshold=0.7
qdrant.search.limit=5

# Logging Configuration
logging.level.root=INFO
logging.level.com.hustfood=DEBUG
logging.level.org.springframework.web=DEBUG
logging.level.org.springframework.security=DEBUG